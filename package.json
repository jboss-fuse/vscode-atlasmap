{
	"name": "atlasmap-viewer",
	"description": "Opens a browser for AtlasMap",
	"version": "0.0.1",
	"publisher": "jboss-fuse",
	"preview": true,
	"bugs": "https://github.com/jboss-fuse/vscode-atlasmap/issues",
	"homepage": "https://github.com/jboss-fuse/vscode-atlasmap",
	"repository": {
		"type": "git",
		"url": "https://github.com/jboss-fuse/vscode-atlasmap.git"
	},
	"contributors": [
		{
			"name": "Aur√©lien Pupier",
			"email": "apupier@redhat.com",
			"url": "http://redhat.com"
		},
		{
			"name": "Lars Heinemann",
			"email": "lheinema@redhat.com",
			"url": "http://redhat.com"
		}
	],
	"license": "Apache-2.0",
	"engines": {
		"vscode": "^1.29.1"
	},
	"keywords": [
		"atlasmap",
		"data",
		"transformation",
		"mapper"
	],
	"categories": [
		"Other"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "atlasmap.start",
				"title": "Open AtlasMap",
				"category": "AtlasMap"
			},
			{
				"command": "atlasmap.stop",
				"title": "Stop AtlasMap",
				"category": "AtlasMap"
			}
		]
	},
	"activationEvents": [
		"onCommand:atlasmap.start",
		"onCommand:atlasmap.stop"
	],
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"clean": "rm -rf out || rmdir out /s /q",
		"preinstall": "node ./scripts/preinstall.js",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run clean && npm run compile && node ./node_modules/vscode/bin/test",
		"update-deps": "npm update",
		"tslint": "gulp tslint"
	},
	"dependencies": {
		"co": "^4.6.0",
		"detect-port": "^1.3.0",
		"expand-home-dir": "^0.0.3",
		"fetch": "^1.1.0",
		"find-java-home": "^0.2.0",
		"opn": "^5.4.0",
		"request": "^2.88.0"
	},
	"devDependencies": {
		"@types/chai": "^4.1.7",
		"@types/mocha": "^5.2.5",
		"@types/node": "^10.12.18",
		"@types/sinon": "^7.0.4",
		"@types/sinon-chai": "^3.2.1",
		"assert": "^1.4.1",
		"chai": "^4.2.0",
		"gulp": "^4.0.0",
		"gulp-tslint": "^8.1.3",
		"mocha": "^5.2.0",
		"mvn-artifact-download": "^4.0.0",
		"sinon": "^7.2.3",
		"sinon-chai": "^3.3.0",
		"tslint": "^5.12.1",
		"typescript": "^3.2.4",
		"vscode": "^1.1.29"
	}
}
