{
	"name": "atlasmap-viewer",
	"displayName": "AtlasMap Data Transformation editor by Red Hat",
	"description": "Opens a browser for AtlasMap Data Transformation editor",
	"version": "0.2.3",
	"publisher": "redhat",
	"icon": "icons/icon128.png",
	"preview": true,
	"config": {
		"atlasmapversion": "2.5.0"
	},
	"bugs": "https://github.com/jboss-fuse/vscode-atlasmap/issues",
	"homepage": "https://github.com/jboss-fuse/vscode-atlasmap",
	"repository": {
		"type": "git",
		"url": "https://github.com/jboss-fuse/vscode-atlasmap.git"
	},
	"contributors": [
		{
			"name": "Aur√©lien Pupier",
			"email": "apupier@redhat.com",
			"url": "https://redhat.com"
		},
		{
			"name": "Lars Heinemann",
			"email": "lheinema@redhat.com",
			"url": "https://redhat.com"
		},
		{
			"name": "Joshi Raez",
			"email": "jraez@redhat.com",
			"url": "https://redhat.com"
		}
	],
	"license": "Apache-2.0",
	"engines": {
		"vscode": "^1.57.0"
	},
	"keywords": [
		"atlasmap",
		"data",
		"transformation",
		"mapper"
	],
	"categories": [
		"Other"
	],
	"main": "./out/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "AtlasMap Settings",
			"properties": {
				"redhat.telemetry.enabled": {
					"type": "boolean",
					"default": null,
					"markdownDescription": "Enable usage data and errors to be sent to Red Hat servers. Read our [privacy statement](https://developers.redhat.com/article/tool-data-collection).",
					"scope": "window",
					"tags": [
						"telemetry"
					]
				}
			}
		},
		"commands": {
			"category": "AtlasMap",
			"title": "Create AtlasMap file",
			"command": "atlasmap.file.create",
			"enablement": "workbenchState != empty"
		},
		"customEditors": [
			{
				"viewType": "atlasmap.editor",
				"displayName": "AtlasMap",
				"selector": [
					{
						"filenamePattern": "*.adm"
					}
				]
			}
		]
	},
	"activationEvents": [
		"workspaceContains:**/*.adm",
		"onLanguage:xml",
		"onLanguage:java",
		"onLanguage:groovy",
		"onLanguage:properties",
		"onLanguage:spring-boot-properties",
		"onLanguage:quarkus-properties",
		"onLanguage:kotlin",
		"onLanguage:javascript",
		"onLanguage:yaml",
		"onCommand:atlasmap.file.create",
		"onCustomEditor:atlasmap.editor"
	],
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./ && npm run lint",
		"watch": "tsc -w -p ./",
		"clean": "rm -rf out || rmdir out /s /q",
		"postinstall": "node ./scripts/postinstall.js",
		"test": "node ./out/test/runTest.js",
		"update-deps": "npm update",
		"ui-test": "extest setup-and-run out/ui-test/all.js --storage uitest-resources --code_settings ./src/ui-test/settings.json --uninstall_extension --code_version 1.73.1",
		"lint": "eslint -c .eslintrc.json --ext .ts src"
	},
	"dependencies": {
		"@redhat-developer/vscode-redhat-telemetry": "^0.5.2",
		"child_process": "^1.0.2",
		"chokidar": "^3.5.2",
		"expand-home-dir": "^0.0.3",
		"find-java-home": "^1.2.2",
		"md5": "^2.3.0",
		"node-fetch": "^2.6.7",
		"path": "^0.12.7",
		"path-exists": "^4.0.0",
		"util": "^0.12.5",
		"valid-filename": "^3.1.0"
	},
	"devDependencies": {
		"@types/chai": "^4.3.4",
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "^18.11.9",
		"@types/node-fetch": "^2.6.2",
		"@types/sinon": "^10.0.12",
		"@types/sinon-chai": "^3.2.9",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.48.1",
		"@vscode/test-electron": "^2.2.0",
		"assert": "^2.0.0",
		"async-wait-until": "^2.0.12",
		"chai": "^4.3.7",
		"eslint": "^8.28.0",
		"eslint-plugin-import": "^2.26.0",
		"file-uri-to-path": "^2.0.0",
		"file-url": "^3.0.0",
		"fs": "^0.0.1-security",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"mocha-jenkins-reporter": "^0.4.8",
		"mvn-artifact-download": "^6.0.0",
		"request": "^2.88.2",
		"sinon": "^15.0.0",
		"sinon-chai": "^3.7.0",
		"typescript": "^4.8.4",
		"vscode-extension-tester": "^5.1.0"
	}
}
